{
  "name": "pokedex",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build-css": "npx tailwindcss -i ./src/index.css -o ./src/styles/output.css --watch",
    "format": "exec prettier . --write",
    "cypress": "cypress open",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown . --write"
  },
  "dependencies": {
    "@apollo/client": "^3.14.0",
    "@tanstack/react-query": "^4.42.0",
    "@tanstack/react-query-devtools": "^4.42.0",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.13.1",
    "graphql": "^16.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "cypress": "^12.17.4",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.5.6",
    "prettier": "3.0.3",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vite": "^4.5.14"
  }
}
